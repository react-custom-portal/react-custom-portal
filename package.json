{
  "name": "react-custom-portal",
  "version": "1.0.6",
  "main": "dist/index.js",
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^10.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist; NODE_ENV=production babel -D -d dist src/components",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepublish": "npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "macro"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-modules-commonjs",
        {
          "noInterop": true,
          "loose": true
        }
      ]
    ]
  },
  "description": "renders part of your component markup outside in some another place",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-custom-portal/react-custom-portal.git"
  },
  "keywords": [
    "react",
    "portal",
    "render",
    "outside"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/react-custom-portal/react-custom-portal/issues"
  }
}
